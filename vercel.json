{
    "version": 2,
    "builds": [
      { "src": "vite.config.ts", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
      { "src": "server/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/contact", "dest": "/server/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  